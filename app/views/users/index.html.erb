
<div class="container">

 
     
          

	  <div class="col-md-9">
     
                <div class="panel-heading">

	<h1>All users</h1>

	</div>
  
     
  <div class="panel-body">
     

   <div class="row">

	<% @users.each do |u| %>
		<div class="col-md-10">
			<h4><%= link_to "#{u.name}", user_path(u.id) %></h4>
			
        </div>
		<div class="col-md-2">
			<img src="<%= u.avatar %>" width="50", class: "avatar-full">
	
		</div>
	<!-- <h4><%= link_to "#{u.job_title}", user_path(u.id) %></h4> -->
	<% end %>


	
	</div>
	</div>
</div>