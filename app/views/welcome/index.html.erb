<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<% if current_user.nil? %>
  <%= link_to 'Sign in', sign_in_path, method: :get %>
<% else %>
  <%= link_to 'Your Account', user_path(current_user), method: :get %>
  <%= link_to 'Sign out', sign_out_path, method: :delete %>
<% end %>
